{
  "redirects": [
    {
      "source": "/skid",
      "destination": "/skid.gif",
      "permanent": true
    },
    {
      "source": "/skid2",
      "destination": "/skid2.gif",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*).gif",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/gif"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
